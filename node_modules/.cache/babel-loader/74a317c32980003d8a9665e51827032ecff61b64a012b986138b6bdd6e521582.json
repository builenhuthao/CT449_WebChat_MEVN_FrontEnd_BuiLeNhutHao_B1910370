{"ast":null,"code":"import axios from \"axios\";\nimport swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      isLoading: false\n    };\n  },\n  methods: {\n    addContact: async function () {\n      const form = event.target;\n      const formData = new FormData(form);\n      this.isLoading = true;\n      const response = await axios.post(this.$apiURL + \"/contact/save\", formData, {\n        headers: this.$headers\n      });\n      this.isLoading = false;\n      swal.fire(\"Thêm bạn mới\", response.data.message, response.data.status);\n      if (response.data.status == \"success\") {\n        form.reset();\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","swal","data","isLoading","methods","addContact","form","event","target","formData","FormData","response","post","$apiURL","headers","$headers","fire","message","status","reset"],"sources":["D:\\Laptrinh\\Phat_trien_ung_dung_web\\Projects\\Web-Chat\\CT449-WebChat-MEVN-FrontEnd-BuiLeNhutHao-B1910370\\src\\components\\AddContactComponent.vue"],"sourcesContent":["<template>\n  <div class=\"container\" style=\"margin-top: 50px\">\n    <div class=\"row\">\n      <div class=\"col-md-12\">\n        <h1 class=\"text-center\">Thêm Bạn Mới</h1>\n      </div>\n    </div>\n\n    <div class=\"row\">\n      <div class=\"offset-md-3 col-md-6\">\n        <form method=\"POST\" v-on:submit.prevent=\"addContact\">\n          <div class=\"form-group\">\n            <label>Name</label>\n            <input type=\"text\" name=\"name\" class=\"form-control\" required />\n          </div>\n\n          <div class=\"form-group\" style=\"margin-top: 20px; margin-bottom: 30px\">\n            <label>E-mail</label>\n            <input type=\"email\" name=\"email\" class=\"form-control\" required />\n          </div>\n\n          <div class=\"d-grid gap-2\">\n            <input\n              type=\"submit\"\n              class=\"btn btn-primary btn-block\"\n              v-bind:value=\"isLoading ? 'Adding...' : 'Add Contact'\"\n              v-bind:disabled=\"isLoading\"\n            />\n          </div>\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport swal from \"sweetalert2\";\n\nexport default {\n  data() {\n    return {\n      isLoading: false,\n    };\n  },\n\n  methods: {\n    addContact: async function () {\n      const form = event.target;\n      const formData = new FormData(form);\n\n      this.isLoading = true;\n      const response = await axios.post(\n        this.$apiURL + \"/contact/save\",\n        formData,\n        {\n          headers: this.$headers,\n        }\n      );\n\n      this.isLoading = false;\n      swal.fire(\"Thêm bạn mới\", response.data.message, response.data.status);\n\n      if (response.data.status == \"success\") {\n        form.reset();\n      }\n    },\n  },\n};\n</script>\n"],"mappings":"AAoCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPC,UAAU,EAAE,eAAAA,CAAA,EAAkB;MAC5B,MAAMC,IAAG,GAAIC,KAAK,CAACC,MAAM;MACzB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAACJ,IAAI,CAAC;MAEnC,IAAI,CAACH,SAAQ,GAAI,IAAI;MACrB,MAAMQ,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAC/B,IAAI,CAACC,OAAM,GAAI,eAAe,EAC9BJ,QAAQ,EACR;QACEK,OAAO,EAAE,IAAI,CAACC;MAChB,EACD;MAED,IAAI,CAACZ,SAAQ,GAAI,KAAK;MACtBF,IAAI,CAACe,IAAI,CAAC,cAAc,EAAEL,QAAQ,CAACT,IAAI,CAACe,OAAO,EAAEN,QAAQ,CAACT,IAAI,CAACgB,MAAM,CAAC;MAEtE,IAAIP,QAAQ,CAACT,IAAI,CAACgB,MAAK,IAAK,SAAS,EAAE;QACrCZ,IAAI,CAACa,KAAK,EAAE;MACd;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}